{
  "name": "chainable-array",
  "version": "1.1.0",
  "description": "More chainable methods to the Array",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test-w": "jest --config jestconfig.json --watchAll",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chikalaka/chainable-array.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "js",
    "ts",
    "typescript",
    "javascript",
    "utils",
    "helpers",
    "vanilla",
    "extend",
    "chainable",
    "array"
  ],
  "author": "Edan Chetrit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chikalaka/chainable-array/issues"
  },
  "homepage": "https://github.com/chikalaka/chainable-array#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
